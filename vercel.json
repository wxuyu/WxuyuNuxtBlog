{
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Content-Type", "value": "application/json" }
      ]
    },
    {
      "source": "/zhilu.opml",
      "headers": [
        { "key": "Content-Type", "value": "application/xml" }
      ]
    },
    {
      "source": "/image/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, s-maxage=31536000, stale-while-revalidate=31536000, immutable" }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, s-maxage=31536000, stale-while-revalidate=31536000, immutable" }
      ]
    },
    {
      "source": "/_nuxt/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, s-maxage=31536000, stale-while-revalidate=31536000, immutable" }
      ]
    },
    {
      "source": "/_payload.json",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, s-maxage=31536000, stale-while-revalidate=31536000, immutable" }
      ]
    },
    {
      "source": "/(.*)/(.*)/(.*)/_payload.json",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, s-maxage=31536000, stale-while-revalidate=31536000, immutable" }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/favicon.ico",
      "destination": "https://www.zhilu.site/api/icon",
      "permanent": true
    }
  ]
}